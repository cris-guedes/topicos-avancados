// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}



model address{
id  String @unique
state String
city String
publicPlace String
neighborhood String
number String
}

model carRegistration{
id  String @unique
plate String 
state String
city String

}

model part{
  id  String @unique
  name String
  email String
  involvement String

}